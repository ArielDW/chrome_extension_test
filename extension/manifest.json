{
	"manifest_version": 3,
	"name": "Ariel's Chrome Extension Test",
	"version": "1.0",
	"description": "Built as a proof of concept. I aint know JS so Im just testing.",
	"permissions": [
		"activeTab",
		"scripting",
		"storage",
		"tabs",
		"clipboardWrite"
	],
	"action": {
		"default_popup": "popup.html"
	},
	"host_permissions": [
		"*://*.wikipedia.org/*",
		"*://samsara.zendesk.com/*",
		"*://cloud.samsara.com/*"
	]
}
